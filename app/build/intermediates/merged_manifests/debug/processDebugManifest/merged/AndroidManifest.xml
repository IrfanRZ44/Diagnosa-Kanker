<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exomatik.irfanrz.diagnosakanker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name="com.exomatik.irfanrz.diagnosakanker.SplashScreen"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.exomatik.irfanrz.diagnosakanker.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.exomatik.irfanrz.diagnosakanker.UserInput"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.exomatik.irfanrz.diagnosakanker.Questioner"
            android:screenOrientation="portrait" />
        <activity android:name="com.exomatik.irfanrz.diagnosakanker.HasilDiagnosa" />
    </application>

</manifest>